int findmax(int array[5], int begin)
{
    int n = begin, tmp = begin;
    while (n < 5)
    {
        if (array[n] > tmp)
        {
            tmp = n;
        }
        n = n + 1;
    }
    return tmp;
}

int sort(int brray[5])
{
    int i = 0;
    int max;
    int tmp;
    while (i < 5)
    {
        max = findmax(brray, i);
        tmp = brray[max];
        brray[max] = brray[i];
        brray[i] = tmp;
        i = i + 1;
    }
    return 0;
}

int main()
{
    int x[2][5];
    int y;
    x[0][0] = 5;
    x[0][1] = 3;
    x[0][2] = 4;
    x[0][3] = 6;
    x[0][4] = 1;
    sort(x[0]);
    write(x[0][0]);
    write(x[0][1]);
    write(x[0][2]);
    write(x[0][3]);
    write(x[0][4]);
    return 0;
}
