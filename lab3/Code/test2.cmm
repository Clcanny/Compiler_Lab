int bubbleSort(int arr[3]) {
    int i = 0, temp = 0, n = 3;
    while (n > 1) 
    {
        i = 0;
        while (i < n - 1) 
        {
            if (arr[i] > arr[i + 1]) 
            {
                write(i);
                temp = arr[i];
                arr[i] = arr[i + 1];
                arr[i + 1] = temp;
            }
            i = i + 1;
        }
        n = n - 1;
    }
    return 0;
}

int main() {
    int array[3];
    int i1 = 0;
    array[0] = 3;
    array[1] = 1;
    array[2] = 2;
    bubbleSort(array);
    while (i1 < 3) {
        write(array[i1]);
        i1 = i1 + 1;
    }
    return 0;
}
