int findmax(int array[5], int begin)
{
    int n = begin, tmp = 0;
    while (n < 5)
    {
        if (array[n] > tmp)
        {
            tmp = array[n];
        }
        n = n + 1;
    }
    return tmp;
}

int main()
{
    int x[5];
    int y;
    x[0] = 5;
    x[1] = 3;
    x[2] = 4;
    x[3] = 6;
    x[4] = 1;
    y = findmax(x, 0);
    write(y);
}
